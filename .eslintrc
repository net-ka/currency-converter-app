{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "jest": true
  },
  "globals": {
    "shallow": true
  },
  "rules": {
    "max-len": [1, 200, 2, {"ignoreComments": true}],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx", ".ts", ".tsx"] }],
    "no-underscore-dangle": [0, { "allow": [] }],
  }
}
